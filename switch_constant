#include<stdio.h>
int main(){
    char ch;
    printf("\nEnter the character:\n");
    scanf(" %c",&ch);
 
    switch(ch){
        case 'a':
           printf (" a is vowel");
           break;
        case 'A':
           printf (" A is vowel");
           break;
        case 'e':
           printf (" e is vowel");
           break;
        case 'E':
           printf (" E is vowel");
           break;
        case 'i':
           printf (" i is vowel");
           break;
