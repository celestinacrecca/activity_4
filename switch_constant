           printf (" O is vowel");
           break;
        case 'u':
           printf (" u is vowel");
           break;
        case 'U':
           printf (" U is vowel");
           break;
        case 'b':
           printf(" b is consonant");
           break;
        case 'B':
           printf(" B is consonant");
           break;
        case 'c':
           printf(" c is consonant");
           break;
        case 'C':
           printf(" C is consonant");
           break;
        case 'd':
           printf(" d is consonant");
           break;
        case 'D':
           printf(" D is consonant");
           break;
        case 'f':
           printf(" f is consonant");
           break;
